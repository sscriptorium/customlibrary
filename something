			function Section_Hyper.update()
				local old = 1
				
				for i, v in ipairs(Section:GetChildren()) do
					if v:IsA('Frame') then
						old += (v.AbsoluteSize.Y or 27) + UIListLayout.Padding.Offset
					end
				end
				
				Tween:Create(Section, TweenInfo.new(0.1), {Size = UDim2.new(TransparentHyper.SectionWidth, 0, 0, old + 1)}):Play()
			end
			
			table.insert(TransparentHyper.AutoUpdate, Section_Hyper.update)
			
			function Section_Hyper.text(...)
				TextLabel.Text = tostring(...)
			end
			
