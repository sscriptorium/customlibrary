		RightFrame.Name = "RightFrame"
		RightFrame.Parent = TabFrame
		RightFrame.Active = true
		RightFrame.AnchorPoint = Vector2.new(0.5, 0.5)
		RightFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
